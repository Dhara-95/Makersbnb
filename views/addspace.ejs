<header>
  <h1>MakersBnb</h1>
</header>

<h1>Add a Space</h1>

<div></div>
<section>
<form action="/addspace" method='post'>

  <!-- consistency of names required (space_name) -->
<input type="text" maxlength="60" name="title" placeholder="Title" required><br />
<br>
<input type="text" name="image" placeholder="Image" required><br />
<br>
<textarea id="description" name="description" placeholder="Description" rows="4" cols="50" maxlength="350">
</textarea><br />
<br>
<input type="number" name="pricePerNight" placeholder="Price per night" required><br />
<br>
<input type="text" name="location" placeholder="Location" required><br />
<br>
<input type="submit" name="Submit">

</form>
</section>
