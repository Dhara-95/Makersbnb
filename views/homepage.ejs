<body>

  <nav class="navbar">
    <div class="purple">

    <div class="container-fluid">
      <div class="navbar-header">
        <img class="logo_homepage" src="/public/images/logo.png" alt="">
        <h1 class="MakersHeading"> MakersBnB </h1>
      </div>
      <ul class="nav navbar-nav">
        <form align="right" action='/addspace' method=“get”>
          <button class="space_button" type=“submit”>Add a Space</button>
        </form>
      </ul>
    </div>
    </div>
  </nav>




<div></div>

 <h1 class="spaces_heading">Spaces</h1>
<container class='container'>
  <% spaces.map((spaces) => { %>
  <a display='left' align='left' href="/space/<%= spaces.id %>">
  <img alt=spaces.image src=<%= spaces.image %> width=270" height="150">
  <div>
  <a display='inline' align='left' href="/space/<%= spaces.id %>"> <%= spaces.title %></a>
  <h2 display='inline' maxlength="100"> <%= spaces.description %></h2>
  <h3 display='inline'> £<%= spaces.pricePerNight %> Per Night</h3>
  </div>
  <br>
  <% }); %>
  </a>
  </container>
